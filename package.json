{
  "name": "react-native-check-update",
  "version": "1.0.0",
  "description": "Ensure your users always stay up-to-date with the latest version of your app. This library offers seamless in-app update functionality for React Native (Android). Fork of react-native-in-app-updates with modern RN support.",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build lib",
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "android",
    "in-app updates",
    "update flow",
    "flexible update",
    "immediate update",
    "app update",
    "mobile updates",
    "react-native-android",
    "lightweight library"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nelson1mr/react-native-check-updates.git"
  },
  "author": "Nelson <nelson1mr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nelson1mr/react-native-check-updates/issues"
  },
  "homepage": "https://github.com/nelson1mr/react-native-check-updates#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.73.1",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.3.1",
    "react-native": "0.76.5",
    "react-native-builder-bob": "^0.32.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  }
}
